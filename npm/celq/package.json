{
  "name": "celq",
  "version": "0.1.1-alpha.5",
  "bin": "lib/index.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc",
    "dev": "yarn build && node lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IvanIsCoding/celq"
  },
  "author": "Ivan Carvalho <ivancarvalho@gatech.edu>",
  "bugs": {
    "url": "https://github.com/IvanIsCoding/celq/issues"
  },
  "homepage": "https://github.com/IvanIsCoding/celq#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "typescript": "^4.9.4"
  },
  "optionalDependencies": {
    "@ivaniscoding/celq-linux-x64": "0.1.1-alpha.5",
    "@ivaniscoding/celq-linux-arm64": "0.1.1-alpha.5",
    "@ivaniscoding/celq-darwin-x64": "0.1.1-alpha.5",
    "@ivaniscoding/celq-darwin-arm64": "0.1.1-alpha.5",
    "@ivaniscoding/celq-windows-x64": "0.1.1-alpha.5",
    "@ivaniscoding/celq-windows-arm64": "0.1.1-alpha.5"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "ignorePatterns": [
      "lib/*"
    ],
    "root": true
  }
}
